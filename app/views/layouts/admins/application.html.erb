<!DOCTYPE html>
<html>
<head>
  <title>SOKKIN</title>
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag 'admins/style', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>

</head>

<body>
  <div class="l-admins">
    <div class="l-admins__sidenav">
      <%= render 'admins/shared/sidenav' %>
    </div>
    <div class="l-admins__content">
      <%= render 'admins/shared/header' %>
      <div class="container-fluid mt-3 mb-4">
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>
